{
    "name": "@awes-io/client",
    "description": "The Awes.io Frontend Framework #vue #nuxt #ui",
    "version": "0.1.0",
    "private": true,
    "author": "Awescode GmbH <info@awescode.de> (https://www.awescode.de)",
    "contributors": [
        "Yevhen L. <y.lisovenko@awescode.de>"
    ],
    "bugs": {
        "url": "https://github.com/awes-io/client/issues"
    },
    "homepage": "https://www.awes.io",
    "workspaces": [
        "packages/*"
    ],
    "keywords": [
        "ui",
        "vue",
        "nuxt",
        "awes-io",
        "material design"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "yarn clean && yarn build-icons && cross-env NODE_ENV=production rollup -c ./build/rollup.config.production.js",
        "build-icons": "node build/icons/font.js && node build/icons/json.js",
        "dev": "rollup -c ./build/rollup.config.js -w",
        "dev-lib": "yarn clean && yarn build-icons && rollup -c ./build/rollup.config.lib.js -w",
        "reload": "browser-sync start -c browser-sync.config.js",
        "watch": "yarn clean && yarn build-icons && concurrently -k --restart-tries=5 --restart-after=500 \"yarn dev\" \"yarn reload\"",
        "lint": "eslint --ext .js,.vue ./src ./tests",
        "lint:fix": "eslint --ext .js,.vue ./src ./tests --fix",
        "test": "jest",
        "prepublishOnly": "yarn run build",
        "test:watch": "jest -w",
        "space": "jest",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@git.awescode.com:awes-io/client.git"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "yarn lint && yarn test"
        }
    },
    "release": {
        "extends": "@semantic-release/gitlab-config",
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogTitle": "Release Notes for Awes.io UI"
                }
            ],
            [
                "@semantic-release/gitlab",
                {
                    "gitlabUrl": "https://git.awescode.com"
                }
            ],
            "@semantic-release/npm",
            [
                "@semantic-release/git",
                {
                    "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
                }
            ]
        ]
    },
    "license": "MIT",
    "dependencies": {
        "autosize": "^4.0.2",
        "axios": "^0.19.0",
        "clipboard-copy": "^3.1.0",
        "core-js": "^2.6.5",
        "dayjs": "^1.8.17",
        "insane": "^2.6.2",
        "loadjs": "^3.6.1",
        "marked": "^0.7.0",
        "popper.js": "^1.15.0",
        "prismjs": "^1.17.1",
        "rambdax": "^3.5.0",
        "tailwindcss": "^1.0.6",
        "tailwindcss-accessibility": "^1.0.0",
        "tailwindcss-typography": "^2.2.0",
        "vue": "^2.6.10",
        "vue-i18n": "^8.14.0",
        "vue-router": "^3.1.2",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@hutson/npm-deploy-git-tag": "^5.0.2",
        "@hutson/semantic-delivery-gitlab": "^9.0.4",
        "@semantic-release/changelog": "^3.0.4",
        "@semantic-release/git": "^7.0.16",
        "@semantic-release/gitlab-config": "^5.0.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "auto-changelog": "^1.14.1",
        "autoprefixer": "^9.6.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "browser-sync": "^2.26.7",
        "chroma-js": "^2.0.6",
        "concurrently": "^4.1.1",
        "conventional-changelog-eslint": "^3.0.1",
        "cross-env": "^5.2.0",
        "cssnano": "^4.1.10",
        "dom-parser": "^0.1.6",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.2.3",
        "husky": "^3.0.1",
        "jest": "^24.8.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "postcss-assets": "^5.0.0",
        "postcss-calc": "^7.0.1",
        "postcss-custom-properties": "^9.0.2",
        "postcss-discard-empty": "^4.0.1",
        "postcss-each": "^0.10.0",
        "postcss-easings": "^2.0.0",
        "postcss-import": "^12.0.1",
