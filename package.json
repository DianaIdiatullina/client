        "jest-junit": "^10.0.0",
        "jsdom": "^15.2.1",
        "klaw-sync": "^6.0.0",
        "lerna": "^3.20.1",
        "lodash": "^4.17.15",
        "node-fetch": "^2.6.0",
        "node-sass": "^4.13.0",
